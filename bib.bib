@article{meighGridBasedApproachWater1999,
  title = {A {{Grid-Based Approach}} to {{Water Scarcity Estimates}} for {{Eastern}} and {{Southern Africa}}},
  author = {Meigh, J. R. and McKenzie, A. A. and Sene, K. J.},
  date = {1999-04-01},
  journaltitle = {Water Resources Management},
  shortjournal = {Water Resources Management},
  volume = {13},
