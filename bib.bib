  title = {Modelling Distances Travelled to Government Health Services in {{Kenya}}},
  author = {Noor, Abdisalan M and Amin, Abdinasir A and Gething, Peter W and Atkinson, Peter M and Hay, Simon I and Snow, Robert W},
