@article{salmeronStochasticOptimizationNatural2010,
  title = {Stochastic {{Optimization}} for {{Natural Disaster Asset Prepositioning}}},
  author = {Salmerón, Javier and Apte, Aruna},
  date = {2010-09-01},
  journaltitle = {Production and Operations Management},
  volume = {19},
