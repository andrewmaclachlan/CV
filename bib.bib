@article{dahdouh-guebasUseRemoteSensing2002,
  title = {The {{Use}} of {{Remote Sensing}} and {{GIS}} in the {{Sustainable Management}} of {{Tropical Coastal Ecosystems}}},
  author = {Dahdouh-Guebas, Farid},
  date = {2002-06-01},
  journaltitle = {Environment, Development and Sustainability},
  shortjournal = {Environment, Development and Sustainability},
  volume = {4},
  number = {2},
  pages = {93--112},
