@article{wangMeasurementOptimizationImpact2012,
  title = {Measurement, {{Optimization}}, and {{Impact}} of {{Health Care Accessibility}}: {{A Methodological Review}}},
  shorttitle = {Measurement, {{Optimization}}, and {{Impact}} of {{Health Care Accessibility}}},
  author = {Wang, Fahui},
