@inproceedings{joseph2018,
  title = {Urban Water and Sanitation in {{Tanzania}} : Remaining Challenges to Providing Safe, Reliable, and Affordable Services for All},
  shorttitle = {Urban Water and Sanitation in {{Tanzania}}},
  author = {Joseph, George and Haque, Sabrina and Ayling, Sophie},
