@article{tunstallSociodemographicDiversityUnexplained2012,
  title = {Socio-Demographic Diversity and Unexplained Variation in Death Rates among the Most Deprived Parliamentary Constituencies in {{Britain}}},
  author = {Tunstall, H. and Mitchell, R. and Gibbs, J. and Platt, S. and Dorling, D.},
  date = {2012-06-01},
