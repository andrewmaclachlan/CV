  author = {Anderson, Jeanne and Martin, M.E. and Smith, M-L. and Dubayah, R.O. and Hofton, M.A. and Hyde, P. and Peterson, B.E. and Blair, J.B. and Knox, R.G.},
  date = {2006-12-15},
