@article{zabelNORTHERNSPOTTEDOWL2003,
  title = {{{NORTHERN SPOTTED OWL HABITAT MODELS FOR RESEARCH AND MANAGEMENT APPLICATION IN CALFORNIA}} ({{USA}})},
  author = {Zabel, Cynthia J. and Dunk, Jeffrey R. and Stauffer, Howard B. and Roberts, Lynn M. and Mulder, Barry S. and Wright, Adrienne},
