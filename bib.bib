@article{tanserMethodologyOptimisingLocation2006,
  title = {Methodology for Optimising Location of New Primary Health Care Facilities in Rural Communities: A Case Study in {{KwaZulu-Natal}}, {{South Africa}}},
  shorttitle = {Methodology for Optimising Location of New Primary Health Care Facilities in Rural Communities},
  author = {Tanser, Frank},
  date = {2006-10-01},
  journaltitle = {Journal of Epidemiology and Community Health},
  shortjournal = {J Epidemiol Community Health},
  volume = {60},
  number = {10},
  eprint = {16973529},
