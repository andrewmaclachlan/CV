@article{murawskiImprovingAccessibilityRural2009,
  title = {Improving Accessibility to Rural Health Services: {{The}} Maximal Covering Network Improvement Problem},
  shorttitle = {Improving Accessibility to Rural Health Services},
  author = {Murawski, Lisa and Church, Richard L.},
  date = {2009-06},
  journaltitle = {Socio-Economic Planning Sciences},
  shortjournal = {Socio-Economic Planning Sciences},
  series = {The Contributions of {{Charles S}}. {{Revelle}}},
  volume = {43},
  number = {2},
  pages = {102--110},
  issn = {0038-0121},
  doi = {10.1016/j.seps.2008.02.012},
  url = {http://www.sciencedirect.com/science/article/pii/S0038012108000207},
  urldate = {2014-04-07},
  abstract = {Accessibility to health facilities is a critical factor in effective health treatment for people in rural areas of lesser-developed countries. In many areas accessibility is diminished by the lack of all-weather roads, making access subject to weather conditions. Location–allocation models have been used to prescribe optimal configurations of health facilities in order to maximize accessibility, but these models are based on the assumption that the underlying transport network is static and always available. Essentially, past work has ignored the potential impacts of improvements to the transport system in modeling access. In this paper we propose a model that treats the opposite side of the location/transport equation; that is, a model that treats existing facility locations as fixed and improves health service accessibility by upgrading links of the transport network to all-weather roads. This new model, called the Maximal Covering Network Improvement Problem (MC-NIP) is formulated as an integer-linear programming problem. An application of the MC-NIP model to the Suhum District of Ghana is presented, which shows that even a modest level of road improvement can lead to substantial increases in all-season access to health service.},
  keywords = {Access,Infrastructure,Network design,Optimization,Planning},
  file = {C\:\\Users\\Andy\\Zotero\\storage\\CFRF2VR6\\Murawski and Church - 2009 - Improving accessibility to rural health services .pdf;C\:\\Users\\Andy\\Zotero\\storage\\QJQUMBH9\\S0038012108000207.html}
}

