@article{devachandraGlobalPositioningSystem2014,
  title = {Global {{Positioning System}} ({{GPS}}) {{Measurements}} of {{Crustal Deformation}} across the {{Frontal Eastern Himalayan Syntaxis}} and {{Seismic}}‐{{Hazard Assessment}}},
