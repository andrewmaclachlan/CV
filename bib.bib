@article{guptaSeismotectonicsLargeEarthquake2014,
  title = {Seismotectonics and Large Earthquake Generation in the {{Himalayan}} Region},
  author = {Gupta, Harsh and Gahalaut, V. K.},
  date = {2014-01},
  journaltitle = {Gondwana Research},
  shortjournal = {Gondwana Research},
  volume = {25},
  number = {1},
  pages = {204--213},
  issn = {1342-937X},
  doi = {10.1016/j.gr.2012.11.006},
  url = {http://www.sciencedirect.com/science/article/pii/S1342937X12003553},
  urldate = {2014-06-04},
  abstract = {Bounded by the western and eastern syntaxes, the Himalayan region has experienced at least five M \textasciitilde{} 8 earthquakes during a seismically very active phase from 1897 through 1952. However, there has been a paucity of M \textasciitilde{} 8 earthquakes since 1952. Examining of various catalogues and seismograms from the Gottingen Observatory, it is established that this quiescence of M \textasciitilde{} 8 earthquakes is real. While it has not been possible to forecast earthquakes, there has been a success in making a medium term forecast of an M 7.3 earthquake in the adjoining Indo-Burmese arc. Similarly we find that in the central Himalayan region, earthquakes of M \&gt; 6.5 have been preceded by seismic swarms and quiescences. In the recent past, based on GPS data, estimates have been made of the accumulated strains and it is postulated that a number of M \textasciitilde{} 8 earthquakes are imminent in the Himalayan region. We examine these estimates and find that while earthquakes of M \textasciitilde{} 8 may occur in the region, however, the available GPS data and their interpretation do not necessarily suggest their size and time of occurrence and whether an earthquake in a particular segment will occur sooner in comparison to that in the neighboring segment. We also comment on the inference of occurrence of M \textasciitilde{} 8 earthquakes based on M8 algorithm for the region. We conclude that while an M \textasciitilde{} 8 earthquake could occur any time anywhere in the Himalayan region, there is no indication as of now as to where and when it would occur. We impress on the need for preparedness to mitigate the pending earthquake disaster in the region.},
  keywords = {Earthquake prediction and forecasting,Great earthquakes,Himalayan seismicity,Quiescence,Seismic gap},
  file = {C\:\\Users\\Andy\\Zotero\\storage\\868KR32E\\Gupta and Gahalaut - 2014 - Seismotectonics and large earthquake generation in.pdf;C\:\\Users\\Andy\\Zotero\\storage\\ZXW7T86B\\S1342937X12003553.html}
