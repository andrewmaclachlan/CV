@article{guanLeveragingPowerMulticore2010,
  title = {Leveraging the Power of Multi-Core Platforms for Large-Scale Geospatial Data Processing: {{Exemplified}} by Generating {{DEM}} from Massive {{LiDAR}} Point Clouds},
  shorttitle = {Leveraging the Power of Multi-Core Platforms for Large-Scale Geospatial Data Processing},
