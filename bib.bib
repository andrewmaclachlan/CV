@article{myneniLargeSeasonalSwings2007,
  title = {Large Seasonal Swings in Leaf Area of {{Amazon}} Rainforests},
  author = {Myneni, Ranga B. and Yang, Wenze and Nemani, Ramakrishna R. and Huete, Alfredo R. and Dickinson, Robert E. and Knyazikhin, Yuri and Didan, Kamel and Fu, Rong and Juárez, Robinson I. Negrón and Saatchi, Sasan S. and Hashimoto, Hirofumi and Ichii, Kazuhito and Shabanov, Nikolay V. and Tan, Bin and Ratana, Piyachat and Privette, Jeffrey L. and Morisette, Jeffrey T. and Vermote, Eric F. and Roy, David P. and Wolfe, Robert E. and Friedl, Mark A. and Running, Steven W. and Votava, Petr and El-Saleous, Nazmi and Devadiga, Sadashiva and Su, Yin and Salomonson, Vincent V.},
  date = {2007-03-20},
  journaltitle = {Proceedings of the National Academy of Sciences},
  shortjournal = {PNAS},
  volume = {104},
  number = {12},
